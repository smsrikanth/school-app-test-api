server:
    port: 9091


spring:
  datasource:
    url: jdbc:postgresql://${SPRING_DATASOURCE_URL}:5432/schoolapp
    driver-class-name: org.postgresql.Driver
    username: ${SPRING_DATASOURCE_USERNAME}
    password: ${SPRING_DATASOURCE_PASSWORD}
  jpa:
    hibernate:
      ddl-auto: update # You can use "update" or "create-drop" depending on your needs
    database-platform: org.hibernate.dialect.PostgreSQLDialect

